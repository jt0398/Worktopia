{
  "name": "mern",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "lint-staged": {
    "*.js": "eslint"
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node server/helpers/dbload.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "concurrently": "^4.1.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "axios": "^0.18.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-fileupload": "^1.1.6-alpha.6",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mdbreact": "^4.22.1",
    "moment": "^2.24.0",
    "mysql2": "^2.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.11.0",
    "react-dates": "^21.4.0",
    "react-dom": "^16.11.0",
    "react-with-direction": "^1.3.1",
    "sequelize": "^5.21.2"
  }
}
